package Items;


import net.minecraft.client.renderer.texture.IconRegister;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;


public class Plasma_Cell extends Item {


	public Plasma_Cell(int id) {
		super(451);
		setCreativeTab(CreativeTabs.tabCombat);
		setMaxStackSize(64);
		setUnlocalizedName(Iteminfo.Plasma_Cell_UNLOCALIZED_NAME);
	}
	 @Override
	  public void registerIcons(IconRegister iconRegister)
	 {
	    itemIcon = iconRegister.registerIcon("futureweapons:plasmacell");
	    }
	 }
