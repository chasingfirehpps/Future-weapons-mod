package Items;


import net.minecraft.client.renderer.texture.IconRegister;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;


public class Plasma_Rifle_Stock extends Item {


	public Plasma_Rifle_Stock(int id) {
		super(452);
		setCreativeTab(CreativeTabs.tabCombat);
		setMaxStackSize(1);
		setUnlocalizedName(Iteminfo.Plasma_Rifle_Stock_UNLOCALIZED_NAME);
	}
	 @Override
	  public void registerIcons(IconRegister iconRegister)
	 {
	    itemIcon = iconRegister.registerIcon("futureweapons:Plasma_Rifle_Stock");
	    }
	 }
